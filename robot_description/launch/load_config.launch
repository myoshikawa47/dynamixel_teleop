<launch>

  <!-- load dynamixel_config -->
  <node name="follower_config_node" pkg="robot_description" type="load_config.py"
      args="--param_name=follower --config=$(find robot_description)/config/follower.yaml"/>

  <node name="leader_config_node" pkg="robot_description" type="load_config.py"
      args="--param_name=leader --config=$(find robot_description)/config/leader.yaml"/>

</launch>
